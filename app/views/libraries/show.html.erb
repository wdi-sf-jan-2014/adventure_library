<h2>Library - <%= @library.url %></h2>
<div class="row">
  <div class="col-md-6">
    <h4>Libraries</h4>
    <% unless @library_list.empty? %>
      <ul>
        <% @library_list.each do |library| %>
          <li><%= link_to(library["url"], library["url"]) %></li>
        <% end %>
      </ul>
    <% end %>
  </div>
  <div class="col-md-6">
    <h4>Adventures</h4>
    <% unless @adventure_list.empty? %>
      <ul>
        <% @adventure_list.each do |adventure| %>
          <li><%= adventure["title"] %></li>
        <% end %>
      </ul>
    <% end %>
  </div>
</div>